
# /etc/sniproxy.conf
```
user daemon
pidfile /var/tmp/sniproxy.pid

error_log {
   # syslog daemon
 filename /var/log/sniproxy/http_error.log

    priority debug
}

resolver {
    nameserver 8.8.4.4 # local dns should be better
    mode ipv4_only
}
listener 80 {
    proto http
	bad_requests log
    access_log {
        filename /var/log/sniproxy/http_access.log
        priority debug
    }
}
listener 443 {
    proto tls
	bad_requests log
    access_log {
        filename /var/log/sniproxy/https_access.log
        priority debug
    }
}

table {
.*disneynow\.com$ *
    (.*\.|)iplocation\.net$ *
.*d9.flashtalking\.com$ *
.*disney-portal.my.onetrust\.com$ *
.*disneyplus.bn5x\.net$ *
